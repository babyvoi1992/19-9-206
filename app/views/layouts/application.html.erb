<!DOCTYPE html>
<html>
<head>
  <title>CoderschoolPrework</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<div class="page-container">
  <div class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">

      <div class="navbar-header">

        <div class="col-xs-6 col-sm-3 sidebar-offcanvas" id="sidebar" role="navigation"  >
          <a class="navbar-brand" style="float:right;padding-right: 5px" href="#" >CoderSchool</a>
        </div>
      </div>
    </div>
  </div>

  <div class="container">
    <div class="row row-offcanvas row-offcanvas-left">

      <!-- sidebar -->
      <div class="col-xs-6 col-sm-3 sidebar-offcanvas" id="sidebar" role="navigation">
        <h2 style="text-transform: uppercase;color: #33acb7; text-align: right; width: 95%;">Website</h2>
        <ul class="nav">

          <li><a href="#">Blog</a></li>
          <li><a href="#">About</a></li>

        </ul>
      </div>

      <!-- main area -->
      <div class="col-xs-12 col-sm-9" style="padding-left: 50px">

        <%= yield %>
      </div><!-- /.col-xs-12 main -->
    </div><!--/.row-->
  </div><!--/.container-->

</div><!--/.page-container-->

</body>
</html>
